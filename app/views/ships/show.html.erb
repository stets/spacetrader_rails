
<h1>My Ships</h1>

<br>
<br>
<br>

  <table>
    <tr>
      <th>Symbol</th>
      <th>Fuel</th>
      <th>Cargo Capacity</th>
    </tr>
    <% @resp["data"].each do |ship| %>
      <tr>
        <td><%= ship["symbol"] %></td>
        <td><%= ship["fuel"]["current"]%> / <%= ship["fuel"]["capacity"] %></td>
        <td><%= ship["cargo"]["capacity"] %></td>
      </tr>
    <% end %>
  </table>

